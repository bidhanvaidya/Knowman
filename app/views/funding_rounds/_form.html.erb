<%= form_for([@company, @fund]) do |f| %>

  <div class="field">
    <%= f.label :date_of_record %><br />
    <%= f.text_field :date_of_record %>
  </div>
  <div class="field">
    <%= f.label :money %><br />
    <%= f.text_field :money %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
